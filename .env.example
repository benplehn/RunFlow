# Supabase connection (replace with your project values)
SUPABASE_PROJECT_REF=your-project-ref
SUPABASE_DB_PASSWORD=super-secret-password
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=dev-jwt-secret-at-least-32-characters

# Local database URL for the optional Supabase Postgres container
DATABASE_URL=postgres://postgres:${SUPABASE_DB_PASSWORD:-super-secret-password}@127.0.0.1:54322/postgres

# Redis connection
REDIS_URL=redis://localhost:6379
