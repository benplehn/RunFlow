{
  "name": "runflow",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write .",
    "db:migrate": "supabase db push --workdir infra/supabase --db-url \"${DATABASE_URL:-postgres://postgres:postgres@localhost:54322/postgres}\"",
    "db:reset": "supabase db reset --workdir infra/supabase --db-url \"${DATABASE_URL:-postgres://postgres:postgres@localhost:54322/postgres}\""
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4"
  }
}
